<?xml version="1.0" encoding="UTF-8"?>
<scenario>
    <send class="Message">
        <message class="Header">
            <field name="type" type="str" value="CALL_CLOSE_REQ"/>
            <field name="tId" type="str" exec="java.util.UUID.randomUUID().toString()"/>
            <field name="msgFrom" type="str" value="AI_AIWF"/>
            <field name="reasonCode" type="int" value="200"/>
            <field name="reason" type="str" value="success"/>
            <field name="timestamp" type="str"
                   exec="java.time.LocalDateTime.now().format(java.time.format.DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd HH:mm:ss.SSS&quot;))"/>
        </message>
        <message class="CallCloseReq">
            <field name="callId" type="str" value="testCallId"/>
        </message>
    </send>

    <recv class="Message">
        <message class="CallCloseRes">

        </message>
    </recv>

    <pause milliseconds="4000"/>

</scenario>